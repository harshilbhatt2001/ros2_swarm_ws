<sdf version='1.7'>
  <world name='default'>

    <!-- Tweak to run at higher or lower speeds -->
    <physics name="normal" type="ode">
        <real_time_update_rate>1000</real_time_update_rate>
        <max_step_size>0.001</max_step_size>
    </physics>
      
    <!-- Ground plane -->
    <include>
        <uri>model://ground_plane</uri>
    </include>
    
    <!-- Lighting -->
    <include>
        <uri>model://sun</uri>
    </include>
    
 </world>
</sdf>
